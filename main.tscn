[sub_resource type="GDScript" id="GDScript_nr0ux"]
script/source = "extends AnimatedSprite2D

var tmp = true
func _process(delta):
	
	if tmp and self.visible:
		var bruitMort = get_node(\"../SFX_mort\")
		bruitMort.play()
		tmp = false
	
	if self.visible==true and Input.is_action_just_released(\"A\"):
		get_tree().change_scene_to_file(\"res://main.tscn\")
		get_tree().paused=false
		
	
		
	elif self.visible == true and Input.is_action_just_released(\"B\"):
		get_tree().quit()
	
	elif self.visible == true and Input.is_action_just_released(\"Y\"):
		get_tree().change_scene_to_file(\"res://sceneMenu.tscn\")
		get_tree().paused=false
"

